<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wh.wanandroid">
    <!-- 访问网络 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WanAndroid">
        <activity
            android:name=".activity.MyCollectActivity"
            android:exported="false"
            android:label="@string/title_activity_my_collect"
            android:theme="@style/Theme.WanAndroid" />
        <activity
            android:name=".activity.ArtiListActivity"
            android:exported="false"
            android:label="@string/title_activity_arti_list"
            android:theme="@style/Theme.WanAndroid" />
        <activity
            android:name=".activity.SyaArtiActivity"
            android:exported="false"
            android:label="@string/title_activity_sya_arti"
            android:theme="@style/Theme.WanAndroid" />
        <activity
            android:name=".activity.SysArtiActivity"
            android:exported="false"
            android:label="@string/title_activity_sys_arti"
            android:theme="@style/Theme.WanAndroid" />
        <activity
            android:name=".activity.AgenWebActivity"
            android:exported="false"
            android:theme="@style/Theme.WanAndroid.NoActionBar" />
        <activity
            android:name=".WebActivity"
            android:exported="false"
            android:label="@string/title_activity_web"
            android:theme="@style/Theme.WanAndroid" />
        <activity
            android:name=".SearchActivity"
            android:exported="false"
            android:label="@string/title_activity_search"
            android:theme="@style/Theme.WanAndroid" />
        <activity
            android:name=".LoginActivity"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.WanAndroid" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>